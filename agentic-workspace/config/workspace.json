{
  "workspace": {
    "name": "agentic-workspace",
    "version": "1.0.0",
    "description": "Comprehensive workspace for AI agent development and deployment"
  },
  "paths": {
    "agents": "./agents",
    "tools": "./tools",
    "workflows": "./workflows",
    "tasks": "./tasks",
    "data": "./data",
    "logs": "./logs",
    "cache": "./data/cache"
  },
  "defaults": {
    "model_provider": "anthropic",
    "model_name": "claude-sonnet-4-5",
    "temperature": 0.7,
    "max_tokens": 4096,
    "timeout_seconds": 300,
    "max_retries": 3
  },
  "logging": {
    "level": "INFO",
    "format": "json",
    "output": ["console", "file"],
    "rotation": {
      "max_size_mb": 100,
      "max_files": 10
    }
  },
  "execution": {
    "parallel_max_workers": 4,
    "enable_caching": true,
    "cache_ttl_seconds": 3600,
    "auto_cleanup": true
  },
  "security": {
    "validate_inputs": true,
    "sanitize_outputs": true,
    "require_agent_auth": false,
    "allowed_tools": "*"
  },
  "monitoring": {
    "enabled": true,
    "metrics": ["execution_time", "success_rate", "error_rate"],
    "alerts": {
      "on_failure": false,
      "on_slow_execution": false
    }
  }
}
